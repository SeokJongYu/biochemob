<%- model_class = Attendee -%>
<div class="page-header">
  <h1>참가자 목록</h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      
      <th>학번</th>
      <th>성명</th>
      
    </tr>
  </thead>
  <tbody>
    <% @attendees.each do |attendee| %>
      <tr>

        <td><%= attendee.sid %></td>
        <td><%= attendee.name %></td>
        
      </tr>
    <% end %>
  </tbody>
</table>